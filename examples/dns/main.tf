terraform {
  required_providers {
    bizflycloud = {
      version = ">= 0.0.5"
      source  = "bizflycloud/bizflycloud"
    }
  }
}

provider "bizflycloud" {
  auth_method = "password"
  region_name = "HaNoi"
  email       = "username"
  password    = ""
}

resource "bizflycloud_dns" "dns_zone" {
  name = "abc.xyz"
}

